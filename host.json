{
  "version": "2.0",
  "logging": {
    "logLevel": {
      "default": "Trace"
    }
  },
  "extensions": {
    "http": {
      "routePrefix": ""
    },
    "queues": {
      "maxPollingInterval": "00:01:00",
      "visibilityTimeout": "00:00:15",
      "batchSize": 1,
      "maxDequeueCount": 5,
      "newBatchThreshold": 1
    },
    "extensionBundle": {
      "id": "Microsoft.Azure.Functions.ExtensionBundle",
      "version": "[3.*, 4.0.0)"
    }
  }
}